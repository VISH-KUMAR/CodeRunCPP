    "compiler",
    "debugger",
    "web",
    "app"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.3",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dockerode": "^3.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
