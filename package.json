{
  "name": "cpp-web-app",
  "version": "1.0.0",
  "description": "Web application to run and debug C++ code",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev-full": "concurrently \"npm run server\" \"npm run client\""
